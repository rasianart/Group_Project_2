<link rel="stylesheet" href="/css/home_styles.css" type="text/css">
<script src="/js/tracking.js"></script>
<script src="/js/splines.js"></script>


<div id="container"></div>

<div class="draw-frame">
    <div id="draw-border"></div>
    <video id="video" width="400" height="300" preload autoplay loop muted></video>
    <canvas id="canvas" width="400" height="300"></canvas>
</div>

<div id="create-hole">Burrow</div>
<div id="dig">Dig</div>


<div id="portal-contain">
    {{#each users}}
        <div id="{{this.name}}" class="portals" data-img="{{this.userimg}}" data-riddle="{{this.riddle}}" data-answer="{{this.answer}}">{{this.name}}</div>
    {{/each}}
</div>

<script src="/js/dat.gui.min.js"></script>
<script src="/js/three.js"></script>
<script src="/js/geometry.js"></script>
<script src="/js/hometracker.js"></script>
<script src="/js/home.js" type="text/javascript"></script>
